void setup()
{
    pinMode(DS6, OUTPUT); // Green
    pinMode(DS5, OUTPUT); // Yellow
    pinMode(DS4, OUTPUT); // Yellow
    pinMode(DS3, OUTPUT); // Yellow
    pinMode(DS2, OUTPUT); // Red
    digitalWrite(DS6, LOW); // Green
    digitalWrite(DS5, LOW); // Yellow
    digitalWrite(DS4, LOW); // Yellow
    digitalWrite(DS3, LOW); // Yellow
    digitalWrite(DS2, LOW); // Red
}

